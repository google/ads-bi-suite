{
  "trdpty_load_reports": {
    "type": "knot",
    "embedded": {
      "tasks": [
        {
          "taskId": "trdpty_trix_import_score",
          "appendedParameters": {
            "datasetId": "ads_reports_data_v4",
            "destinationTable": "trdpty_scoreboard",
            "sqlSyntax": "SELECT * FROM ads_reports_data_v4.trdpty_scoreboard_input"
          }
        }
      ]
    }
  },
  "trdpty_trix_import_score": {
    "type": "query",
    "source": {
      "sql": "${sqlSyntax}",
      "external": true
    },
    "destination": {
      "table": {
        "projectId": "#PROJECT_ID#",
        "datasetId": "${datasetId}",
        "tableId": "${destinationTable}$${partitionDay}"
      },
      "writeDisposition": "WRITE_TRUNCATE"
    }
  }
}
