  "base_account_budget": {
    "type": "report",
    "source": {
      "target": "ADS",
      "config": {
        "developerToken": "${developerToken}",
        "customerId": "${customerId}",
        "loginCustomerId": "${loginCustomerId}",
        "reportQuery": {
          "entity": "account_budget",
          "attributes": [
            "customer.id",
            "customer.currency_code",
            "customer.descriptive_name",
            "account_budget.status",
            "account_budget.approved_spending_limit_micros",
            "account_budget.approved_start_date_time",
            "account_budget.approved_end_date_time",
            "account_budget.adjusted_spending_limit_micros",
            "account_budget.amount_served_micros"
          ]
        }
      }
    },
    "destination": {
      "bucket": "#GCS_BUCKET#",
      "name": "reports/${partitionDay}/reports_base_account_budget/${customerId}.ndjson"
    },
    "errorOptions": {
      "ignoreError": true
    }
  },
  "base_account_performance": {
    "type": "report",
    "source": {
      "target": "ADS",
      "config": {
        "developerToken": "${developerToken}",
        "customerId": "${customerId}",
        "loginCustomerId": "${loginCustomerId}",
        "reportQuery": {
          "entity": "customer",
          "metrics": [
            "metrics.cost_micros",
            "metrics.impressions",
            "metrics.clicks",
            "metrics.conversions",
            "metrics.conversions_value",
            "metrics.all_conversions",
            "metrics.all_conversions_value",
            "metrics.video_views"
          ],
          "segments": ["segments.date"],
          "attributes": [
            "customer.id",
            "customer.currency_code",
            "customer.descriptive_name",
            "customer.optimization_score"
          ],
          "date_constant": "LAST_30_DAYS"
        }
      }
    },
    "destination": {
      "bucket": "#GCS_BUCKET#",
      "name": "reports/${partitionDay}/reports_base_account_performance/${customerId}.ndjson"
    },
    "errorOptions": {
      "ignoreError": true
    }
  },
  "base_campaigns": {
    "type": "report",
    "source": {
      "target": "ADS",
      "config": {
        "developerToken": "${developerToken}",
        "customerId": "${customerId}",
        "loginCustomerId": "${loginCustomerId}",
        "reportQuery": {
          "entity": "campaign",
          "attributes": [
            "customer.id",
            "customer.currency_code",
            "customer.descriptive_name",
            "campaign.id",
            "campaign.name",
            "campaign.advertising_channel_type",
            "campaign.advertising_channel_sub_type",
            "campaign_budget.amount_micros",
            "campaign.status",
            "campaign.app_campaign_setting.app_id",
            "campaign.app_campaign_setting.app_store",
            "campaign.target_cpa.target_cpa_micros",
            "campaign.app_campaign_setting.bidding_strategy_goal_type",
            "campaign.optimization_score",
            "campaign.bidding_strategy",
            "campaign.bidding_strategy_type",
            "campaign.target_roas.target_roas"
          ],
          "constraints": [
            {
              "key": "campaign.status",
              "op": "IN",
              "val": ["PAUSED", "ENABLED"]
            }
          ]
        }
      }
    },
    "destination": {
      "bucket": "#GCS_BUCKET#",
      "name": "reports/${partitionDay}/reports_base_campaigns/${customerId}.ndjson"
    },
    "errorOptions": {
      "ignoreError": true
    }
  },
  "base_campaign_ads_approval": {
    "type": "report",
    "source": {
      "target": "ADS",
      "config": {
        "developerToken": "${developerToken}",
        "customerId": "${customerId}",
        "loginCustomerId": "${loginCustomerId}",
        "reportQuery": {
          "entity": "ad_group_ad",
          "segments": [],
          "attributes": [
            "customer.id",
            "customer.descriptive_name",
            "campaign.id",
            "campaign.name",
            "campaign.status",
            "campaign.advertising_channel_type",
            "campaign.advertising_channel_sub_type",
            "ad_group.id",
            "ad_group.name",
            "ad_group.status",
            "ad_group_ad.ad.id",
            "ad_group_ad.ad.name",
            "ad_group_ad.ad.type",
            "ad_group_ad.policy_summary.approval_status",
            "ad_group_ad.policy_summary.policy_topic_entries",
            "ad_group_ad.policy_summary.review_status"
          ],
          "constraints": [
            {
              "key": "ad_group_ad.status",
              "op": "NOT IN",
              "val": ["PAUSED", "REMOVED"]
            },
            {
              "key": "ad_group.status",
              "op": "NOT IN",
              "val": ["PAUSED", "REMOVED"]
            }
          ]
        }
      }
    },
    "destination": {
      "bucket": "#GCS_BUCKET#",
      "name": "reports/${partitionDay}/reports_base_campaign_ads_approval/${customerId}.ndjson"
    },
    "errorOptions": {
      "ignoreError": true
    }
  },
  "base_campaign_conversion": {
    "type": "report",
    "source": {
      "target": "ADS",
      "config": {
        "developerToken": "${developerToken}",
        "customerId": "${customerId}",
        "loginCustomerId": "${loginCustomerId}",
        "reportQuery": {
          "entity": "campaign",
          "metrics": [
            "metrics.conversions",
            "metrics.conversions_value",
            "metrics.all_conversions_value",
            "metrics.all_conversions"
          ],
          "segments": [
            "segments.date",
            "segments.ad_network_type",
            "segments.conversion_action_category",
            "segments.conversion_action_name",
            "segments.conversion_action",
            "segments.external_conversion_source"
          ],
          "attributes": [
            "customer.id",
            "campaign.id",
            "campaign.advertising_channel_type"
          ],
          "date_constant": "LAST_30_DAYS"
        }
      }
    },
    "destination": {
      "bucket": "#GCS_BUCKET#",
      "name": "reports/${partitionDay}/reports_base_campaign_conversion/${customerId}.ndjson"
    },
    "errorOptions": {
      "ignoreError": true
    }
  },
  "base_campaign_performance": {
    "type": "report",
    "source": {
      "target": "ADS",
      "config": {
        "developerToken": "${developerToken}",
        "customerId": "${customerId}",
        "loginCustomerId": "${loginCustomerId}",
        "reportQuery": {
          "entity": "campaign",
          "metrics": [
            "metrics.cost_micros",
            "metrics.impressions",
            "metrics.clicks",
            "metrics.conversions",
            "metrics.conversions_value",
            "metrics.all_conversions",
            "metrics.all_conversions_value",
            "metrics.video_views"
          ],
          "segments": [
            "segments.date",
            "segments.ad_network_type",
            "segments.device"
          ],
          "attributes": [
            "customer.id",
            "customer.currency_code",
            "customer.descriptive_name",
            "campaign.id",
            "campaign.name",
            "campaign.status",
            "campaign.advertising_channel_type",
            "campaign.advertising_channel_sub_type",
            "campaign.campaign_budget"
          ],
          "constraints": [
            {
              "key": "metrics.impressions",
              "op": ">=",
              "val": 1
            }
          ],
          "date_constant": "LAST_30_DAYS"
        }
      }
    },
    "destination": {
      "bucket": "#GCS_BUCKET#",
      "name": "reports/${partitionDay}/reports_base_campaign_performance/${customerId}.ndjson"
    },
    "errorOptions": {
      "ignoreError": true
    }
  },
  "base_detail_placement_view": {
    "type": "report",
    "source": {
      "target": "ADS",
      "config": {
        "developerToken": "${developerToken}",
        "customerId": "${customerId}",
        "loginCustomerId": "${loginCustomerId}",
        "reportQuery": {
          "entity": "detail_placement_view",
          "metrics": [
            "metrics.cost_micros",
            "metrics.impressions",
            "metrics.clicks",
            "metrics.conversions",
            "metrics.conversions_value",
            "metrics.all_conversions",
            "metrics.all_conversions_value",
            "metrics.video_views"
          ],
          "attributes": [
            "customer.id",
            "customer.currency_code",
            "customer.descriptive_name",
            "campaign.id",
            "campaign.name",
            "campaign.status",
            "campaign.advertising_channel_type",
            "campaign.advertising_channel_sub_type",
            "detail_placement_view.display_name",
            "detail_placement_view.placement",
            "detail_placement_view.placement_type",
            "detail_placement_view.target_url"
          ],
          "constraints": [
            {
              "key": "campaign.status",
              "op": "=",
              "val": "ENABLED"
            },
            {
              "key": "metrics.clicks",
              "op": ">=",
              "val": 1
            },
            {
              "key": "metrics.impressions",
              "op": ">=",
              "val": 100
            },
            {
              "key": " campaign.advertising_channel_type",
              "op": "NOT IN",
              "val": ["SEARCH", "SHOPPING"]
            }
          ],
          "date_constant": "LAST_30_DAYS",
          "order_by": "metrics.impressions",
          "sort_order": "DESC",
          "limit": 1000
        }
      }
    },
    "destination": {
      "bucket": "#GCS_BUCKET#",
      "name": "reports/${partitionDay}/reports_base_detail_placement_view/${customerId}.ndjson"
    },
    "errorOptions": {
      "ignoreError": true
    }
  },
